#!/bin/bash
#SBATCH -p alldlc_gpu-rtx2080 # bosch_cpu-cascadelake # partition (queue)
#SBATCH --mem 72000 # memory pool for all cores (4GB)
#SBATCH -t 1-00:00 # time (D-HH:MM)
#SBATCH -c 4 # number of cores
#SBATCH -o log/%x.%N.%j.out # STDOUT  (the folder log has to be created prior to running or this won't work)
#SBATCH -e log/%x.%N.%j.err # STDERR  (the folder log has to be created prior to running or this won't work)
#SBATCH -J test_cma_ALL # sets the job name. If not specified, the file name will be used as job name
#SBATCH --mail-type=END,FAIL # (recive mails about end and timeouts/crashes of your job)
# Print some information about the job to STDOUT
source ~/.bashrc
conda activate dacbench
echo "Workingdir: $PWD";
echo "Started at $(date)";
echo "Running job $SLURM_JOB_NAME using $SLURM_JOB_CPUS_PER_NODE cpus per node with given JID $SLURM_JOB_ID on queue $SLURM_JOB_PARTITION"; 
# Job to perform
export TF_XLA_FLAGS=--tf_xla_cpu_global_jit
python3 source/gps/gps_test.py DAC_Journal
# Print some Information about the end-time to STDOUT
echo "DONE";
echo "Finished at $(date)";
