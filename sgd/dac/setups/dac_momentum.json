{
  "train_env": {
    "obj_class": "DACBenchWrapper",
    "module": "sgd.dac.bench",
    "kwargs": {
      "benchmark": {
        "obj_class": "SGDBenchmark",
        "module": "dacbench.benchmarks",
        "kwargs": {
          "instance_set_path": "../instance_sets/sgd/sgd_train_100instances.csv",
          "optimizer": "momentum",
          "cd_paper_reconstruction": true,
          "cd_bias_correction": true,
          "training_batch_size": 64,
          "train_validation_ratio": 1.0,
          "features": [
            "predictiveChangeVarDiscountedAverage",
            "predictiveChangeVarUncertainty",
            "lossVarDiscountedAverage",
            "lossVarUncertainty",
            "currentLR",
            "trainingLoss",
            "step"
        ]
        }
      },
      "policy_space": {
        "obj_class": "MomentumPolicy",
        "module": "sgd.dac.policies"
      }
    }
  },
  "val_env": {
    "obj_class": "DACBenchWrapper",
    "module": "sgd.dac.bench",
    "kwargs": {
      "benchmark": {
        "obj_class": "SGDBenchmark",
        "module": "dacbench.benchmarks",
        "kwargs": {
          "instance_set_path": "../instance_sets/sgd/sgd_train_100instances.csv",
          "optimizer": "momentum",
          "cd_paper_reconstruction": true,
          "cd_bias_correction": true,
          "training_batch_size": 64,
          "train_validation_ratio": 1.0,
          "features": [
            "predictiveChangeVarDiscountedAverage",
            "predictiveChangeVarUncertainty",
            "lossVarDiscountedAverage",
            "lossVarUncertainty",
            "currentLR",
            "trainingLoss",
            "step"
        ]
        }
      },
      "policy_space": {
        "obj_class": "MomentumPolicy",
        "module": "sgd.dac.policies"
      }
    }
  },
  "deterministic": true,
  "param_scale": {
    "obj_class": "SymLogScale",
    "module": "sgd.cfg_util",
    "kwargs": {
      "vx_min": 0.01,
      "vx_max": 1.0,
      "vy_min": 0.000001,
      "vy_max": 10.0
    }
  },
  "result_dir": "results/dac_momentum3",
  "cache_evaluations": true,
  "overwrite": true,
  "trials_train_limit": 5000,
  "seed": "$1"
}
